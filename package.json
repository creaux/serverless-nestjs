{
  "name": "@creaux/tpl-nest",
  "version": "0.0.1",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "nodemon:dev": "nodemon",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --passWithNoTests",
    "test:e2e": "jest --config ./test/jest-e2e.json --passWithNoTests"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.4",
    "@nestjs/core": "^7.6.4",
    "@nestjs/platform-express": "^7.6.4",
    "@nestjs/swagger": "^4.7.8",
    "@creaux/lib-common": "1.0.8",
    "aws-sdk": "^2.816.0",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.4",
    "@types/aws-lambda": "^8.10.67",
    "@types/express": "^4.17.9",
    "@types/jest": "26.0.19",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "serverless-offline": "^6.8.0",
    "supertest": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
